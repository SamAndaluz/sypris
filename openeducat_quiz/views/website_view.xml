<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Over all result view -->
        <template id="exam_user_result_profile" name="Exam Main View">
            <t t-call="website.layout">
                <div class="container mt32">
                    <div class="col-md-12">
                        <t t-call="openeducat_quiz.quiz_profile"/>
                    </div>
                    <div class="col-md-12">
                        <table class="table table-bordered mt16">
                            <thead>
                                <tr>
                                    <th>Exam Name</th>
                                    <th>Total Question</th>
                                    <th>Total Correct</th>
                                    <th>Total Incorrect</th>
                                    <th>Total Marks</th>
                                    <th>Received Marks</th>
                                    <th>Score</th>
                                </tr>
                            </thead>
                            <tbody>
                                <t t-foreach="result_data" t-as="line">
                                    <tr>
                                        <td>
                                            <t t-esc="line['name']"/>
                                        </td>
                                        <td>
                                            <t t-esc="line['ttl_que']"/>
                                        </td>
                                        <td>
                                            <t t-esc="line['ttl_crct']"/>
                                        </td>
                                        <td>
                                            <t t-esc="line['ttl_incrct']"/>
                                        </td>
                                        <td>
                                            <t t-esc="line['ttl_marks']"/>
                                        </td>
                                        <td>
                                            <t t-esc="line['rec_marks']"/>
                                        </td>
                                        <td>
                                            <t t-esc="line['score']"/>
                                        </td>
                                    </tr>
                                </t>
                            </tbody>
                        </table>
                    </div>
                </div>
            </t>
        </template>

        <!-- Exam Main Page -->
        <template id="online_exam_page" name="Exam Main View">
            <t t-call="website.layout">
                <t t-call="openeducat_quiz.quiz_render_exam_main_form_view"/>
            </t>
        </template>

        <template id="quiz_render_exam_main_form_view" name="Exam View">
            <div class="container mt32">
                <div class="row">
                    <t t-call="openeducat_quiz.quiz_profile"/>
                </div>
                <div class="row">
                    <div class="mb48 col-lg-12 col-md-12 col-sm-12">
                        <div class="row">
                            <div class="col-lg-2 col-md-2 col-sm-2">
                                <center>
                                    <img t-attf-src="/web/image/res.users/#{user.id}/image_1920"
                                         class="img-fluid img-circle"/>
                                </center>
                            </div>
                            <div class="col-lg-10 col-md-10 col-sm-10">
                                <div class="row">
                                    <div class="mb32 col-lg-12 col-md-12 col-sm-12">
                                        <h3>
                                            <b>
                                                <span t-esc="user.name"/>
                                            </b>
                                        </h3>
                                    </div>
                                    <div class="col-lg-6 col-md-6 col-sm-6">
                                        <h4>
                                            <b>Total Attempt</b>
                                            :
                                            <span t-esc="total_exam"/>
                                        </h4>
                                    </div>
                                    <div class="col-lg-6 col-md-6 col-sm-6">
                                        <h4>
                                            <b>Average Result</b>
                                            :
                                            <span id="progressBar" style="margin-left: +10px; padding: 5px 10px;"
                                                  t-esc="progress"/>
                                        </h4>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <t t-foreach="exams" t-as="exam">
                        <div class="col-md-4 col-sm-4">
                            <div class="feature">
                                <div class="card card-body bg-primary-light well1 mb-4" style="border-radius: 10px;">
                                    <div class="feature-content-box">
                                        <center>
                                            <h3 class="feature_title mb20">
                                                <strong>
                                                    <span t-esc="exam.name"/>
                                                </strong>
                                            </h3>
                                        </center>
                                        <div style="height: 70px;">
                                            <p style="height: Auto;">
                                                <span t-esc="exam.description"/>
                                            </p>
                                        </div>
                                        <hr/>
                                        <div class="row pl-5">
                                            <div class="col-md-5">Attempt :
                                                <span t-esc="quiz_attempt[exam.id]['ttl_atmp']"/>
                                            </div>
                                            <div class="col-md-7">Avg Result :
                                                <span t-esc="quiz_attempt[exam.id]['avg_res']"/>
                                                %
                                            </div>
                                        </div>
                                        <hr/>
                                        <div t-if="quiz_attempt[exam.id]['allow'] == 1">
                                            <center>
                                                <a class="btn btn-primary btn-lg"
                                                   t-attf-href="/exam/start/#{slug(exam)}">Start Exam
                                                </a>
                                            </center>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </t>
                </div>


            </div>
        </template>

        <template id="quiz_profile" name="Users Profile">

        </template>

        <template id="quiz_render_form_view" name="Quiz Attempt Form">
            <t t-call="website.layout">
                <t t-call="openeducat_quiz.quiz_render_single_form_view"/>
            </t>
        </template>

        <template id="quiz_starting_page">
            <t t-call="website.layout">
                <section>
                    <div class="container">
                        <div class="row mt-4 mb-4">
                            <div class="col-md-12">
                                <center>
                                    <h1 class="float-left">
                                        <span style="color: green" t-esc="exam.name"/>
                                    </h1>
                                </center>
                                <div class="col-md-12" t-if="exam.quiz_audio and audio">
                                    <center>
                                        <h3>Please hear the audio before start exam</h3>
                                    </center>
                                    <span t-raw="audio"/>
                                </div>
                                <div class="col-md-12" t-if="exam.quiz_video and video">
                                    <center>
                                        <h3>Please see the video before start exam</h3>
                                    </center>
                                    <span t-raw="video"/>
                                </div>
                                <div class="col-md-12 float-left" t-if="exam.quiz_html and html">
                                    <span t-raw="exam.quiz_html"/>
                                </div>
                                <a t-if="single_page == 0" class="btn btn-primary btn-lg quiz_prv float-right"
                                   t-attf-href="/quiz/attempt/#{slug(result)}">Start Exam
                                </a>
                                <a t-if="single_page == 1" class="btn btn-primary btn-lg quiz_prv float-right"
                                   t-attf-href="/quiz/#{slug(result)}">Start Exam
                                </a>
                            </div>
                        </div>
                    </div>
                </section>
            </t>
        </template>

        <template id="quiz_render_single_form_view">
            <form action="/quiz/attempt/record" method="post" class="form-horizontal mt32 mb32" id="from_quiz_dynamic">
                <input type="hidden" name="csrf_token" t-att-value="request.csrf_token()"/>
                <a id="submit_exam" t-attf-href="/quiz/submit/#{slug(line.result_id)}" style="display: none;"></a>
                <input type="hidden" id="time_hr" name="time_hr" t-attf-value="#{time_hr}"/>
                <input type="hidden" id="time_minute" name="time_minute" t-attf-value="#{time_minute}"/>
                <input type="hidden" id="time_spent_hr" name="time_spent_hr" t-attf-value="#{time_spent_hr}"/>
                <input type="hidden" id="time_spent_minute" name="time_spent_minute"
                       t-attf-value="#{time_spent_minute}"/>
                <input type="hidden" id="time_spent_second" name="time_spent_second"
                       t-attf-value="#{time_spent_second}"/>
                <div class="container">
                    <div class="mt16 mb16">
                        <h1 class="text-center">
                            <t t-esc="exam.name"/>
                        </h1>
                    </div>
                    <div class="row">
                        <div class="col-md-9">
                            <div class="form-group" style="font-size: 20px;">
                                <h2><t t-raw="question_no"/>)
                                    <t t-raw="line.name"/>
                                </h2>
                                <input type="hidden" t-attf-name="question" t-att-value="line.id"/>
                                <t t-if="line.que_type == 'blank'">
                                    <t t-if="is_required ==  1">
                                        <input t-if="is_readonly == 1" type="text" t-attf-name="answer"
                                               t-attf-value="#{line.given_answer}" style="width: 50%;" required="True"
                                               disabled="true"/>
                                        <input t-if="is_readonly == 0" type="text" t-attf-name="answer"
                                               t-attf-value="#{line.given_answer}" style="width: 50%;" required="True"/>
                                    </t>
                                    <t t-if="is_required ==  0">
                                        <input t-if="is_readonly == 1" type="text" t-attf-name="answer"
                                               t-attf-value="#{line.given_answer}" style="width: 50%;" disabled="true"/>
                                        <input t-if="is_readonly == 0" type="text" t-attf-name="answer"
                                               t-attf-value="#{line.given_answer}" style="width: 50%;"/>
                                    </t>
                                </t>
                                <t t-if="line.que_type == 'optional'">
                                    <t t-if="is_required == 1 and is_readonly == 1" t-foreach="line.line_ids"
                                       t-as="option">
                                        <input t-if="option.id == given_answer"
                                               checked="checked" type="radio"
                                               t-attf-name="answer"
                                               t-att-id="'que%sopt%s'%(line.id,option.id)"
                                               t-attf-value="#{option.id}"
                                               required="true" disabled="true"/>
                                        <t t-if="option.id == given_answer" t-esc="option.name"/>
                                        <input t-if="option.id != given_answer"
                                               type="radio" t-attf-name="answer"
                                               t-att-id="'que%sopt%s'%(line.id,option.id)"
                                               t-attf-value="#{option.id}"
                                               required="true" disabled="true"/>
                                        <t t-if="option.id != given_answer" t-esc="option.name"/>
                                        <br/>
                                    </t>
                                    <t t-if="is_required == 1 and is_readonly == 0"
                                       t-foreach="line.line_ids" t-as="option">
                                        <input t-if="option.id == given_answer"
                                               checked="checked" type="radio"
                                               t-attf-name="answer"
                                               t-att-id="'que%sopt%s'%(line.id,option.id)"
                                               t-attf-value="#{option.id}"
                                               required="true"/>
                                        <t t-if="option.id == given_answer" t-esc="option.name"/>
                                        <input t-if="option.id != given_answer"
                                               type="radio" t-attf-name="answer"
                                               t-att-id="'que%sopt%s'%(line.id,option.id)"
                                               t-attf-value="#{option.id}"
                                               required="true"/>
                                        <t t-if="option.id != given_answer" t-esc="option.name"/>
                                        <br/>
                                    </t>
                                    <t t-if="is_required == 0 and is_readonly == 1"
                                       t-foreach="line.line_ids" t-as="option">
                                        <input t-if="option.id == given_answer"
                                               checked="checked" type="radio"
                                               t-attf-name="answer"
                                               t-att-id="'que%sopt%s'%(line.id,option.id)"
                                               t-attf-value="#{option.id}"
                                               disabled="true"/>
                                        <t t-if="option.id == given_answer" t-esc="option.name"/>
                                        <input t-if="option.id != given_answer"
                                               type="radio" t-attf-name="answer"
                                               t-att-id="'que%sopt%s'%(line.id,option.id)"
                                               t-attf-value="#{option.id}"
                                               disabled="true"/>
                                        <t t-if="option.id != given_answer" t-esc="option.name"/>
                                        <br/>
                                    </t>
                                    <t t-if="is_readonly == 0 and is_required == 0" t-foreach="line.line_ids"
                                       t-as="option">
                                        <input t-if="option.id == given_answer"
                                               checked="checked" type="radio"
                                               t-attf-name="answer"
                                               t-att-id="'que%sopt%s'%(line.id,option.id)"
                                               t-attf-value="#{option.id}"/>
                                        <t t-if="option.id == given_answer" t-esc="option.name"/>
                                        <input t-if="option.id != given_answer"
                                               type="radio" t-attf-name="answer"
                                               t-att-id="'que%sopt%s'%(line.id,option.id)"
                                               t-attf-value="#{option.id}"/>
                                        <t t-if="option.id != given_answer" t-esc="option.name"/>
                                        <br/>
                                    </t>
                                </t>
                                <t t-if="line.que_type == 'descriptive'">
                                    <t t-if="is_required ==  1">
                                        <textarea t-if="is_readonly == 1" type="textarea" rows="8" style="width:100%;"
                                                  t-attf-name="answer" t-attf-value="#{line.given_answer}"
                                                  required="True" disabled="true"/>
                                        <textarea t-if="is_readonly == 0" type="textarea" rows="8" style="width:100%;"
                                                  t-attf-name="answer" t-attf-value="#{line.given_answer}"
                                                  required="True"/>
                                    </t>
                                    <t t-if="is_required ==  0">
                                        <textarea t-if="is_readonly == 1" type="textarea" rows="8" style="width:100%;"
                                                  t-attf-name="answer" t-attf-value="#{line.given_answer}"
                                                  disabled="true"/>
                                        <textarea t-if="is_readonly == 0" type="textarea" rows="8" style="width:100%;"
                                                  t-attf-name="answer" t-attf-value="#{line.given_answer}"/>
                                    </t>
                                </t>
                            </div>
                            <div class="col-md-7 col-sm-8">
                                <a t-if="prev_allow == 1 and timer == 0"
                                   class="btn btn-primary btn-lg quiz_prv"
                                   t-attf-href="/quiz/attempt/#{slug(line.result_id)}/question/#{slug(prev_result)}">
                                    Prev
                                </a>
                                <a t-if="prev_allow == 1 and timer == 1" id="prev_timer"
                                   class="btn btn-primary btn-lg quiz_prv"
                                   t-attf-href="/quiz/attempt/#{slug(line.result_id)}/question/#{slug(prev_result)}/prev/">
                                    Prev
                                </a>
                                <input type="hidden" name="ExamID" t-attf-value="#{exam.id}"/>
                                <button t-if="next_allow == 1" class="btn btn-primary btn-lg quiz_finish1">Next</button>
                                <button id="final_finish" t-if="next_allow == 0"
                                        class="btn btn-primary btn-lg quiz_finish1">Finish
                                </button>
                            </div>
                        </div>
                        <div class="col-md-3 mb32"
                             style="border: 1px solid;padding: 10px">
                            <div class="col-md-12 mb32">
                                <div t-if="timer == 1" id="divCounter" class="text-center">
                                    <span t-if="timer == 1" id="all_time" style="display:none;"/>
                                    <center>
                                        <table>
                                            <tr>
                                                <th>
                                                    <center>Hr</center>
                                                </th>
                                                <th>
                                                    <center>Min</center>
                                                </th>
                                                <th>
                                                    <center>Sec</center>
                                                </th>
                                            </tr>
                                            <tr>
                                                <td>
                                                    <center>
                                                        <span class="heading-digit" id="spanHr" style="width:40px;"/>
                                                    </center>
                                                </td>
                                                <td>
                                                    <center>
                                                        <span class="heading-digit" id="spanMt" style="width:40px;"/>
                                                    </center>
                                                </td>
                                                <td>
                                                    <center>
                                                        <span class="heading-digit" id="spanSn" style="width:40px;"/>
                                                    </center>
                                                </td>
                                            </tr>
                                        </table>
                                    </center>
                                    <!--<span class="heading-digit" id="spanHr"/>-->
                                    <!--<span class="heading-digit" id="spanMt"/>-->
                                    <!--<span class="heading-digit" id="spanSn"/>-->
                                </div>
                                <center>
                                    <h4 class="mb16">Question Grid</h4>
                                </center>
                                <t t-foreach="grid_data" t-as="grid_val">
                                    <a t-attf-href="#"
                                       t-if="grid_val['is_ans_given'] == 1 and grid_val['is_readonly'] == 1 and grid_val['current_id'] == 0">
                                        <div class="btn btn-default col-md-2"
                                             disabled="true"
                                             style="background-color: #4CAF50;">
                                            <span t-esc="grid_val['que_no']"/>
                                        </div>
                                    </a>
                                    <a t-attf-href="/quiz/attempt/#{slug(line.result_id)}/question/#{slug(grid_val['line'])}"
                                       t-if="grid_val['is_ans_given'] == 1 and grid_val['is_readonly'] == 0 and grid_val['current_id'] == 0">
                                        <div class="btn btn-default col-md-2"
                                             style="background-color: #4CAF50;">
                                            <span t-esc="grid_val['que_no']"/>
                                        </div>
                                    </a>
                                    <a t-attf-href="/quiz/attempt/#{slug(line.result_id)}/question/#{slug(grid_val['line'])}"
                                       t-if="grid_val['is_ans_given'] == 0 and grid_val['is_readonly'] == 0 and grid_val['current_id'] == 0">
                                        <div class="btn btn-default col-md-2">
                                            <span t-esc="grid_val['que_no']"/>
                                        </div>
                                    </a>
                                    <a t-attf-href="#"
                                       t-if="grid_val['is_ans_given'] == 0 and grid_val['is_readonly'] == 1 and grid_val['current_id'] == 0">
                                        <div class="btn btn-default col-md-2"
                                             disabled="true">
                                            <span t-esc="grid_val['que_no']"/>
                                        </div>
                                    </a>
                                    <a t-attf-href="/quiz/attempt/#{slug(line.result_id)}/question/#{slug(grid_val['line'])}"
                                       t-if="grid_val['current_id'] == 1">
                                        <div class="btn btn-default col-md-2"
                                             style="background-color: #357EBD;">
                                            <span t-esc="grid_val['que_no']"/>
                                        </div>
                                    </a>
                                </t>
                            </div>
                            <br/>
                            <div class="col-md-12">
                                <span class="progress-value" t-raw="progress"/>
                                <div id="progressBar">
                                    <div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </form>
        </template>

        <template id="quiz_web_page" name="Quiz Questions">
            <t t-call="website.layout">
                <form action="/quiz/results" method="post" class="form-horizontal mt32" id="from_quiz">
                    <input type="hidden" name="csrf_token" t-att-value="request.csrf_token()"/>
                    <div class="wrap">
                        <div class="container">
                            <div class="mt16 mb16">
                                <h1 class="text-center">
                                    <t t-esc="exam.name"/>
                                </h1>
                            </div>
                            <div>
                                <t t-foreach="result.line_ids" t-as="question">
                                    <div class="form-group" style="font-size: 20px;">
                                        <h2><t t-esc="question_index + 1"/>)
                                            <t t-raw="question.name"/>
                                        </h2>
                                        <t t-if="question.que_type == 'optional'">
                                            <t t-foreach="question.line_ids" t-as="option">
                                                <input type="radio"
                                                       t-attf-name="question#{question.id}"
                                                       t-att-id="'que%sopt%s'%(question.id,option.id)"
                                                       t-attf-value="#{option.id}"/>
                                                <t t-esc="option.name"/>
                                                <br/>
                                            </t>
                                        </t>
                                        <t t-if="question.que_type == 'blank'">
                                            <input type="text" t-attf-name="blank#{question.id}" style="width: 50%;"/>
                                        </t>
                                        <t t-if="question.que_type == 'descriptive'">
                                            <textarea type="textarea" t-attf-name="descriptive#{question.id}" rows="8"
                                                      style="width:100%;"/>
                                        </t>
                                    </div>
                                </t>
                                <div class="form-group">
                                    <div class="col-md-7 col-sm-8">
                                        <input type="hidden" name="ExamID" t-attf-value="#{result.id}"/>
                                        <button class="btn btn-primary btn-lg quiz_finish">Finish</button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
            </t>
        </template>

        <template id="quiz_web_page_single" name="Quiz Questions">
            <t t-call="website.layout">
                <t t-call="openeducat_quiz.quiz_web_page_single_form"/>
            </t>
        </template>

        <template id="quiz_web_page_single_form" name="Quiz Questions Form View">
            <form action="/quiz/results" method="post" class="form-horizontal mt32" id="from_quiz">
                <input type="hidden" name="csrf_token" t-att-value="request.csrf_token()"/>
                <div class="wrap">
                    <div class="container">
                        <div class="mt16 mb16">
                            <h1 class="text-center">
                                <t t-esc="exam.name"/>
                            </h1>
                        </div>
                        <div id="quiz_err_info"/>
                        <div>
                            <div class="col-md-9">
                                <t t-foreach="result.line_ids" t-as="question">
                                    <div t-if="question_index == 0">
                                        <t t-call="openeducat_quiz.quiz_first_question"/>
                                    </div>
                                    <div t-if="question_index != 0 and question_index != (total_question - 1)">
                                        <t t-call="openeducat_quiz.quiz_middle_question"/>
                                    </div>
                                    <div t-if="question_index == (total_question - 1)">
                                        <t t-call="openeducat_quiz.quiz_last_question"/>
                                    </div>
                                </t>
                            </div>
                            <div class="col-md-3" style="border: 1px solid;padding: 10px">
                                <center>
                                    <h4>Question Grid</h4>
                                </center>
                                <t t-foreach="result.line_ids" t-as="question">
                                    <div t-att-class="'btn btn-default col-md-2 que_grid que_active' if question_index == 0 else 'btn btn-default col-md-2 que_grid'"
                                         t-att-grid-index-id="'%s' % (question_index)">
                                        <span t-esc="question_index + 1"/>
                                    </div>
                                </t>
                            </div>
                        </div>
                    </div>
                </div>
            </form>
        </template>

        <template id="quiz_first_question" name="quiz_first_question">
            <div class="form-group que_show question" style="font-size: 20px;" t-att-index-id="'%s' % (question_index)">
                <h2><t t-esc="question_index + 1"/>)
                    <t t-esc="question.name"/>
                </h2>
                <t t-foreach="question.line_ids" t-as="option">
                    <input type="radio" t-attf-name="question#{question.id}"
                           t-att-id="'que%sopt%s'%(question.id,option.id)"
                           t-attf-value="#{option.id}"/>
                    <t t-esc="option.name"/>
                    <br/>
                </t>
                <a class="btn btn-primary btn-lg quiz_nxt" t-att-next-id="'%s' % (question_index + 1)" align="right">
                    Next
                </a>
            </div>
        </template>

        <template id="quiz_middle_question" name="quiz_middle_question">
            <div class="form-group que_hide question" style="font-size: 20px;" t-att-index-id="'%s' % (question_index)">
                <h2><t t-esc="question_index + 1"/>)
                    <t t-esc="question.name"/>
                </h2>
                <t t-foreach="question.line_ids" t-as="option">
                    <input type="radio" t-attf-name="question#{question.id}"
                           t-att-id="'que%sopt%s'%(question.id,option.id)"
                           t-attf-value="#{option.id}"/>
                    <t t-esc="option.name"/>
                    <br/>
                </t>
                <a class="btn btn-primary btn-lg quiz_prv" t-att-prev-id="'%s' % (question_index - 1)" align="left">
                    Prev
                </a>
                <a class="btn btn-primary btn-lg quiz_nxt" t-att-next-id="'%s' % (question_index + 1)" align="right">
                    Next
                </a>
            </div>
        </template>

        <template id="quiz_last_question" name="quiz_last_question">
            <div class="form-group que_hide question" style="font-size: 20px;" t-att-index-id="'%s' % (question_index)">
                <h2><t t-esc="question_index + 1"/>)
                    <t t-esc="question.name"/>
                </h2>
                <t t-foreach="question.line_ids" t-as="option">
                    <input type="radio" t-attf-name="question#{question.id}"
                           t-att-id="'que%sopt%s'%(question.id,option.id)"
                           t-attf-value="#{option.id}"/>
                    <t t-esc="option.name"/>
                    <br/>
                </t>
                <div>
                    <a class="btn btn-primary btn-lg quiz_prv" t-att-prev-id="'%s' % (question_index - 1)">Prev</a>
                    <button class="btn btn-primary btn-lg quiz_finish">
                        <input type="hidden" name="ExamID" t-attf-value="#{result.id}"/>
                        Finish
                    </button>
                </div>
            </div>
        </template>

        <template id="quiz_completed" name="Quiz Completed">
            <t t-call="website.layout">
                <div class="wrap">
                    <div class="container">
                        <center>
                            <h1>Thank you for Quiz</h1>
                        </center>
                        <center>
                            <h2>Your answers are recorded</h2>
                        </center>
                    </div>
                </div>
            </t>
        </template>

        <template id="quiz_results" name="Quiz Results">
            <t t-call="website.layout">
                <t t-call="openeducat_quiz.quiz_results_form"/>
            </t>
        </template>

        <template id="quiz_results_form" name="Quiz Results Form">
            <div class="wrap">
                <div class="container">
                    <div class="mt16 mb16">
                        <h1 class="text-center">Score Board</h1>
                    </div>
                    <div class="mt16 mb16">
                        <table class="table table-bordered">
                            <thead>
                                <tr>
                                    <th>
                                        <center>Total Questions</center>
                                    </th>
                                    <th>
                                        <center>Total Correct</center>
                                    </th>
                                    <th>
                                        <center>Total Incorrect</center>
                                    </th>
                                    <th>
                                        <center>Total Marks</center>
                                    </th>
                                    <th>
                                        <center>Received Marks</center>
                                    </th>
                                    <th>
                                        <center>Result</center>
                                    </th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td>
                                        <center>
                                            <t t-esc="total_question"/>
                                        </center>
                                    </td>
                                    <td>
                                        <center>
                                            <t t-esc="total_correct"/>
                                        </center>
                                    </td>
                                    <td>
                                        <center>
                                            <t t-esc="total_incorrect"/>
                                        </center>
                                    </td>
                                    <td>
                                        <center>
                                            <t t-esc="total_marks"/>
                                        </center>
                                    </td>
                                    <td>
                                        <center>
                                            <t t-esc="received_marks"/>
                                        </center>
                                    </td>
                                    <td>
                                        <center><t t-esc="percentage"/>%
                                        </center>
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                        <t t-if="is_message == 1">
                            <span t-raw="message" class="mb16"/>
                        </t>
                        <div t-if="display_wrong_ans == 1">
                            <div class="mt16 mb16">
                                <h1 class="text-center">Wrongly Given Answers</h1>
                            </div>
                            <t>
                                <table class="table table-bordered">
                                    <thead>
                                        <tr>
                                            <th style="background-color: 'green';">
                                                <center>Question</center>
                                            </th>
                                            <th>
                                                <center>Given Aswer</center>
                                            </th>
                                            <th>
                                                <center>Original Answer</center>
                                            </th>
                                        </tr>
                                    </thead>
                                    <t t-foreach="wrong_answer" t-as="answer">
                                        <tbody>
                                            <tr>
                                                <td>
                                                    <center>
                                                        <t t-esc="answer['question']"/>
                                                    </center>
                                                </td>
                                                <td>
                                                    <center>
                                                        <t t-esc="answer['given_answer']"/>
                                                    </center>
                                                </td>
                                                <td>
                                                    <center>
                                                        <t t-esc="answer['answer']"/>
                                                    </center>
                                                </td>
                                            </tr>
                                        </tbody>
                                    </t>
                                </table>
                            </t>
                        </div>
                        <div t-if="not_attempt_ans == 1">
                            <div class="mt16 mb16">
                                <h1 class="text-center">Not Attempt Answers</h1>
                            </div>
                            <t>
                                <table class="table table-bordered">
                                    <thead>
                                        <tr>
                                            <th style="background-color: 'green';">
                                                <center>Question</center>
                                            </th>
                                            <th>
                                                <center>Answer</center>
                                            </th>
                                        </tr>
                                    </thead>
                                    <t t-foreach="not_attempt_answer" t-as="answer">
                                        <tbody>
                                            <tr>
                                                <td>
                                                    <center>
                                                        <t t-esc="answer['question']"/>
                                                    </center>
                                                </td>
                                                <td>
                                                    <center>
                                                        <t t-esc="answer['answer']"/>
                                                    </center>
                                                </td>
                                            </tr>
                                        </tbody>
                                    </t>
                                </table>
                            </t>
                        </div>
                        <div t-if="display_true_ans == 1">
                            <div class="mt16 mb16">
                                <h1 class="text-center">Truly Given Answers</h1>
                            </div>
                            <t>
                                <table class="table table-bordered">
                                    <thead>
                                        <tr>
                                            <th style="background-color: 'green';">
                                                <center>Question</center>
                                            </th>
                                            <th>
                                                <center>Answer</center>
                                            </th>
                                        </tr>
                                    </thead>
                                    <t t-foreach="right_answers" t-as="answer">
                                        <tbody>
                                            <tr>
                                                <td>
                                                    <center>
                                                        <t t-esc="answer['question']"/>
                                                    </center>
                                                </td>
                                                <td>
                                                    <center>
                                                        <t t-esc="answer['answer']"/>
                                                    </center>
                                                </td>
                                            </tr>
                                        </tbody>
                                    </t>
                                </table>
                            </t>
                        </div>
                    </div>
                </div>
            </div>
        </template>
    </data>
</odoo>
